#include <stdio.h>

int main() {
 int num1,num2,num3;
 printf("Max number of three numbers\n");
 printf("____________________________\n");
 printf("enter the first value:");
 scanf("%d",&num1);
 printf("enter the secound value:");
 scanf("%d",&num2);
 printf("enter the third value:");
 scanf("%d",&num3);
 
 if (num1 >= num2 && num1 >= num3){
     printf("maxium number of %d ",num1);
 }
 else if (num1 >= num2 && num2 >= num3){
     printf("Maxium number of %d",num2);
    
 }
 else{
     printf("Maxum number is %d",num3);
 }
    return 0;
}
